        case 0: parlante=2; sound_global_volume(1);
            if(mision=0){sound_loop(snd_fondo0);}
            else{sound_loop(snd_fondo1);} break;
        case 1: parlante=0; sound_global_volume(0); break;
        case 2: parlante=1;
            if(sound_isplaying(snd_fondo0)){sound_stop(snd_fondo0);}
            else if(sound_isplaying(snd_fondo1)){sound_stop(snd_fondo1);} break;}}
if(keyboard_check_released(vk_end) and room_speed&gt;20){room_speed-=1;}
else if(keyboard_check_released(vk_home) and room_speed&lt;40){room_speed+=1;}
