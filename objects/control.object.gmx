    if(mision=0){
        if(show_question("Desea Abandonar el Juego?")){game_end();}}
    else{
        if(instance_exists(unidad)){
            if(show_question("Desea Ir al Men√∫ del Juego?")){reiniciando();}}
        else{alarm[0]=1;}}}
